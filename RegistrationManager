#include <iostream>
using namespace std;

class RegistrationManager {
  public:
    string managerRegistrationInfo[6];
    string registrationDB[9];
    int voterID;
    string form;
    void genID();
    void storeInfo();
    int returnSuccess();
};

void RegistrationManager::genID(){
  voterID = rand() % 10000;
}

void RegistrationManager::storeInfo(){
  registrationDB[0] = voterID;
  registrationDB[1] = form;
  for( int i = 2; i < 9; i++ ){
    int j = 0;
    registrationDB[i] = managerRegistrationInfo[j];
    j++;
}

int RegistrationManager::returnSuccess(){
  return voterID;
}
